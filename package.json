{
  "name": "react-open-weather-map-starter",
  "version": "1.0.0",
  "description": "This is a project to start development with open weather map API.",
  "main": "./index.js",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "convert-units": "^2.3.4",
    "cross-fetch": "^2.1.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "reactstrap": "^5.0.0-beta.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.20.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "fetch-mock": "^6.3.0",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^1.5.0",
    "json-loader": "^0.5.7",
    "react-hot-loader": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "test": "jest --verbose",
    "build": "webpack",
    "watch": "webpack -w",
    "dev": "webpack-dev-server --hot --inline --env.NODE_ENV=development",
    "start-dev": "concurrently \"npm run watch\" \"npm run dev\"",
    "start": "npm run build && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemavidal/react-open-weather-map-starter.git"
  },
  "keywords": [
    "react",
    "webpack",
    "open-weather-map"
  ],
  "author": "Hem√£ Vidal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hemavidal/react-open-weather-map-starter/issues"
  },
  "homepage": "https://github.com/hemavidal/react-open-weather-map-starter#readme"
}
